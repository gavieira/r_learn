@article{Wetterer2013,
abstract = {*[The samsum or sword ant, Pachycondyla (Brachyponera) sennaarensis (Mayr, 1862), is a widespread, conspicuous ant in the savannas {\&} open forests of sub-Saharan Africa {\&} the Middle East. This ant is also common in villages and cities, where it is well known for its powerful sting that sometimes leads to anaphylactic shock in humans {\&} even death. Recent first reports of P. sennaarensis from subtropical parts of the Arabian Peninsula {\&} Iran have led many researchers to conclude that populations of this ant are expanding. To evaluate this possibility, I compiled {\&} mapped specimen records of P. sennaarensis from more-than 300 sites. I documented the earliest known P. sennaarensis records for 40 countries, including several for which I found no previously published records: Bahrain, Chad, India, Liberia, Mozambique, {\&} Zambia. In 1925, when most of the world's ant fauna was still poorly documented, P. sennaarensis had already been collected from widely scattered locales in 26 countries across tropical Africa {\&} Arabia. In addition, there is a single isolated 1896 record of P. sennaarensis from an apparently temporary population in tropical India. All P. sennaarensis records from subtropical sites north of 23.4-degrees-N (in Bahrain, Kuwait, Iran, Oman, Qatar, the United Arab Emirates, {\&} Saudi Arabia) are from after 1950, except one record from just outside the tropics (Muscat, Oman; 23.6-degrees-N). It is possible that P. sennaarensis populations have lived in these subtropical areas for a long time, but are only now being reported, perhaps due to a recent population increase of this species. In fact, although the earliest specimen record from Iran dates to 2001, older residents of SE Iran remember the sting of P. sennaarensis from their childhood. Still, it is possible that P. sennaarensis has really expanded its geographic range northeastward into new regions during the past few decades.]},
author = {Wetterer, James K.},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wetterer - 2013 - Geographic spread of the samsum or sword ant, Pachycondyla (Brachyponera) senna-arensis (Hymenoptera Formicidae).pdf:pdf},
issn = {19944136},
journal = {Myrmecological News},
keywords = {Biogeography,Biological invasion,Pest ants,Range expansion},
number = {March 2013},
pages = {13--18},
title = {{Geographic spread of the samsum or sword ant, Pachycondyla (Brachyponera) senna-arensis (Hymenoptera: Formicidae)}},
volume = {18},
year = {2013}
}
@article{West2013,
abstract = {Gender disparities appear to be decreasing in academia according to a number of metrics, such as grant funding, hiring, acceptance at scholarly journals, and productivity, and it might be tempting to think that gender inequity will soon be a problem of the past. However, a large-scale analysis based on over eight million papers across the natural sciences, social sciences, and humanities reveals a number of understated and persistent ways in which gender inequities remain. For instance, even where raw publication counts seem to be equal between genders, close inspection reveals that, in certain fields, men predominate in the prestigious first and last author positions. Moreover, women are significantly underrepresented as authors of single-authored papers. Academics should be aware of the subtle ways that gender disparities can occur in scholarly authorship.},
archivePrefix = {arXiv},
arxivId = {arXiv:1211.1759v1},
author = {West, Jevin D. and Jacquet, Jennifer and King, Molly M. and Correll, Shelley J. and Bergstrom, Carl T.},
