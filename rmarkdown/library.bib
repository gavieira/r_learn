journal = {Molecular Ecology},
keywords = {behavior/social evolution,eusocial insects,molecular evolution,phenotypic plasticity,transcriptomics},
number = {1},
pages = {26--28},
pmid = {24372753},
title = {{The importance of genomic novelty in social evolution}},
volume = {23},
year = {2014}
}
