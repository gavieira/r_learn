@article{Kutschera2005,
abstract = {In 1905, the Russian biologist C. Mereschkowsky postulated that plastids (e.g., chloroplasts) are the evolutionary descendants of endosymbiotic cyanobacteria-like organisms. In 1927, I. Wallin explicitly postulated that mitochondria likewise evolved from once free-living bacteria. Here, we summarize the history of these endosymbiotic concepts to their modern-day derivative, the "serial endosymbiosis theory", which collectively expound on the origin of eukaryotic cell organelles (plastids, mitochondria) and subsequent endosymbiotic events. Additionally, we review recent hypotheses about the origin of the nucleus. Model systems for the study of "endosymbiosis in action" are also described, and the hypothesis that symbiogenesis may contribute to the generation of new species is critically assessed with special reference to the secondary and tertiary endosymbiosis (macroevolution) of unicellular eukaryotic algae. {\textcopyright} 2005 Elsevier GmbH. All rights reserved.},
author = {Kutschera, U. and Niklas, K. J.},
doi = {10.1016/j.thbio.2005.04.001},
isbn = {1431-7613 (Print)$\backslash$r1431-7613 (Linking)},
issn = {14317613},
journal = {Theory in Biosciences},
keywords = {Algae,Chloroplasts,Cyanobacteria,Endosymbiosis,Mitochondria,Plastid evolution,Speciation},
number = {1},
pages = {1--24},
pmid = {17046345},
title = {{Endosymbiosis, cell evolution, and speciation}},
volume = {124},
year = {2005}
}
