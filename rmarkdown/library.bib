file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wetterer - 2012 - Worldwide spread of the moorish sneaking ant, cardiocondylamauritanicn (hymenoptera Formicidae).pdf:pdf},
issn = {03616525},
journal = {Sociobiology},
keywords = {Biogeography,Biological invasion,Exotic species,Invasive species},
number = {3},
pages = {985--997},
title = {{Worldwide spread of the moorish sneaking ant, cardiocondylamauritanicn (hymenoptera: Formicidae)}},
