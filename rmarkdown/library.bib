pmid = {20388197},
title = {{Improving draft assemblies by iterative mapping and assembly of short reads to eliminate gaps}},
url = {http://genomebiology.biomedcentral.com/articles/10.1186/gb-2010-11-4-r41},
volume = {11},
year = {2010}
}
