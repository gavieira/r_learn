issn = {0305-1048},
journal = {Nucleic Acids Research},
number = {4},
pages = {gkw955},
pmid = {27924028},
title = {{NOVOPlasty: de novo assembly of organelle genomes from whole genome data}},
url = {https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkw955},
volume = {45},
year = {2016}
}
