@article{Nagarajan2013a,
abstract = {Previously the province of large genome centres, DNA sequencing is now a key component of research carried out in many laboratories. A number of new applica-tions of sequencing technologies have been spurred by rapidly decreasing costs, including the study of microbial communities (metagenomics), the discovery of structural variants in genomes and the analysis of gene structure and expression. The length of the sequences generated by modern sequencing instru-ments is considerably shorter (hundreds to thousands of base pairs) than that of the genomes or genomic features being studied (which commonly span tens of thousands to billions of base pairs). Thus, many analyses start with the computational process of sequence assembly that joins together the many sequence fragments generated by the instrument. Biologists who need to assemble the sequencing data generated in their experiments are faced with the challenge of choosing, from a myriad of options, the assembly strategy and software best suited for their experiment. This choice is made harder by the rapid development of new assembly tools, which is driven by advances in sequencing technologies and the broader scope of applications for these technolo-gies. Among recent innovations in sequence assem-bly are the use of memory-efficient data structures},
author = {Nagarajan, Niranjan and Pop, Mihai},
doi = {10.1038/nrg3367},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Nagarajan, Pop - 2013 - Sequence assembly demystified.pdf:pdf},
journal = {Nature Reviews Genetics},
title = {{Sequence assembly demystified}},
volume = {14},
year = {2013}
}
@book{Baumert2016,
abstract = {Prolonged unaccustomed exercise involving muscle lengthening (eccentric) actions can result in ultrastructural muscle disruption, impaired excitation-contraction coupling, inflammation and muscle protein degradation. This process is associated with delayed onset muscle soreness and is referred to as exercise-induced muscle damage. Although a certain amount of muscle damage may be necessary for adaptation to occur, excessive damage or inadequate recovery from exercise-induced muscle damage can increase injury risk, particularly in older individuals, who experience more damage and require longer to recover from muscle damaging exercise than younger adults. Furthermore, it is apparent that inter-individual variation exists in the response to exercise-induced muscle damage, and there is evidence that genetic variability may play a key role. Although this area of research is in its infancy, certain gene variations, or polymorphisms have been associated with exercise-induced muscle damage (i.e. individuals with certain genotypes experience greater muscle damage, and require longer recovery, following strenuous exercise). These polymorphisms include ACTN3 (R577X, rs1815739), TNF (-308 G{\textgreater}A, rs1800629), IL6 (-174 G{\textgreater}C, rs1800795), and IGF2 (ApaI, 17200 G{\textgreater}A, rs680). Knowing how someone is likely to respond to a particular type of exercise could help coaches/practitioners individualise the exercise training of their athletes/patients, thus maximising recovery and adaptation, while reducing overload-associated injury risk. The purpose of this review is to provide a critical analysis of the literature concerning gene polymorphisms associated with exercise-induced muscle damage, both in young and older individuals, and to highlight the potential mechanisms underpinning these associations, thus providing a better understanding of exercise-induced muscle damage.},
author = {Baumert, Philipp and Lake, Mark J. and Stewart, Claire E. and Drust, Barry and Erskine, Robert M.},
booktitle = {European Journal of Applied Physiology},
doi = {10.1007/s00421-016-3411-1},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Baumert et al. - 2016 - Genetic variation and exercise-induced muscle damage implications for athletic performance, injury and ageing.pdf:pdf},
isbn = {0042101634111},
issn = {14396319},
keywords = {Creatine kinase,Delayed onset muscle soreness,Elderly,Exercise-induced muscle damage,Single nucleotide polymorphism},
number = {9},
pages = {1595--1625},
pmid = {27294501},
publisher = {Springer Berlin Heidelberg},
title = {{Genetic variation and exercise-induced muscle damage: implications for athletic performance, injury and ageing}},
volume = {116},
year = {2016}
}
