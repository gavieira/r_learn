doi = {10.1016/j.molbiopara.2017.11.005},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Deb et al. - 2018 - Analysis of codon usage pattern of mitochondrial protein-coding genes in different hookworms.pdf:pdf},
issn = {18729428},
journal = {Molecular and Biochemical Parasitology},
keywords = {AT content,Codon usage bias,GC content,Mutation pressure,Natural selection},
month = {jan},
