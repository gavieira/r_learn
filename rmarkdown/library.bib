journal = {Trends in ecology {\&} evolution},
keywords = {Animals,Biodiversity,Classification,Classification: methods,DNA,DNA: chemistry,Databases, Genetic,Diet,Ecology,Ecology: trends,Food Chain,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Species Specificity},
