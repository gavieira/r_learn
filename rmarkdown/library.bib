file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sharma, Vander Meer, Fadamiro - 2011 - Phorid fly, Pseudacteon tricuspis, response to alkylpyrazine analogs of a fire ant, Solenopsis in.pdf:pdf},
isbn = {0022-1910},
issn = {00221910},
journal = {Journal of Insect Physiology},
keywords = {2-Ethyl-3,6-dimethyl pyrazine,Alarm pheromone,Alkylpyrazine analogs,Electroantennogram (EAG),Olfactometer,Pseudacteon tricuspis,Solenopsis invicta},
number = {7},
