@article{DeQueiroz2007,
abstract = {Recent reviews of the construction of large phylogenies have focused on supertree methods that involve separate analyses of data sets and subsequent integration of the resulting trees. Here, we consider the alternative method of analyzing all character data simultaneously. Such 'supermatrix' analyses use information from each character directly and enable straightforward incorporation of diverse kinds of data, including characters from fossils. The approach has been extended by the development of new methods, including model-based techniques for analyzing heterogeneous data and hierarchical methods for constructing extremely large trees. Recent work also suggests that the problem of missing data in supermatrix analyses has been overstated. Although the supermatrix approach is not suited for all cases, we suggest that its inherent strengths will ensure that it will continue to have a central role in inferring large phylogenetic trees from diverse data. {\textcopyright}2006 Elsevier Ltd. All rights reserved.},
author = {de Queiroz, Alan and Gatesy, John},
doi = {10.1016/j.tree.2006.10.002},
isbn = {0169-5347 (Print){\$}\backslash{\$}r0169-5347 (Linking)},
issn = {01695347},
journal = {Trends in Ecology and Evolution},
number = {1},
pages = {34--41},
pmid = {17046100},
title = {{The supermatrix approach to systematics}},
volume = {22},
year = {2007}
}
@article{Kutschera2005,
abstract = {In 1905, the Russian biologist C. Mereschkowsky postulated that plastids (e.g., chloroplasts) are the evolutionary descendants of endosymbiotic cyanobacteria-like organisms. In 1927, I. Wallin explicitly postulated that mitochondria likewise evolved from once free-living bacteria. Here, we summarize the history of these endosymbiotic concepts to their modern-day derivative, the "serial endosymbiosis theory", which collectively expound on the origin of eukaryotic cell organelles (plastids, mitochondria) and subsequent endosymbiotic events. Additionally, we review recent hypotheses about the origin of the nucleus. Model systems for the study of "endosymbiosis in action" are also described, and the hypothesis that symbiogenesis may contribute to the generation of new species is critically assessed with special reference to the secondary and tertiary endosymbiosis (macroevolution) of unicellular eukaryotic algae. {\textcopyright}2005 Elsevier GmbH. All rights reserved.},
author = {Kutschera, U and Niklas, K J},
doi = {10.1016/j.thbio.2005.04.001},
isbn = {1431-7613 (Print){\$}\backslash{\$}r1431-7613 (Linking)},
issn = {14317613},
journal = {Theory in Biosciences},
keywords = {Algae,Chloroplasts,Cyanobacteria,Endosymbiosis,Mitochondria,Plastid evolution,Speciation},
number = {1},
pages = {1--24},
pmid = {17046345},
title = {{Endosymbiosis, cell evolution, and speciation}},
volume = {124},
year = {2005}
}
