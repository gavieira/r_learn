@article{Blagoderov2012,
abstract = {Traditional approaches for digitizing natural history collections, which include both imaging and metadata capture, are both labour- and time-intensive. Mass-digitization can only be completed if the resource-intensive steps, such as specimen selection and databasing of associated information, are minimized. Digitization of larger collections should employ an "industrial" approach, using the principles of automation and crowd sourcing, with minimal initial metadata collection including a mandatory persistent identifier. A new workflow for the mass-digitization of natural history museum collections based on these principles, and using SatScan{\textregistered}tray scanning system, is described.},
author = {Blagoderov, Vladimir and Kitching, Ian J and Livermore, Laurence and Simonsen, Thomas J and Smith, Vincent S},
doi = {10.3897/zookeys.209.3178},
isbn = {1313297013132989},
