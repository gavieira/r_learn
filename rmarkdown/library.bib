file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Krogh et al. - Unknown - Predicting Transmembrane Protein Topology with a Hidden Markov Model Application to Complete Genomes.pdf:pdf},
