@article{Schurch2016,
abstract = {RNA-seq is now the technology of choice for genome-wide differential gene expression experiments, but it is not clear how many biological replicates are needed to ensure valid biological interpretation of the results or which statistical tools are best for analyzing the data. An RNA-seq experiment with 48 biological replicates in each of two conditions was performed to answer these questions and provide guidelines for experimental design. With three biological replicates, nine of the 11 tools evaluatedfound only 20{\%}-40{\%} of the significantly differentially expressed (SDE) genes identified with the full set of 42 clean replicates. This rises to {\textgreater}85{\%} for the subset of SDE genes changing in expression by more than fourfold. To achieve {\textgreater}85{\%} for all SDE genes regardless of fold change requires more than 20 biological replicates. The same nine tools successfully control their false discovery rate at ≲5{\%} for all numbers of replicates, while the remaining two tools fail to control their FDR adequately, bparticularly for low numbers of replicates. For future RNA-seq experiments, these results suggest that at least six biological replicates should be used, rising to at least 12 when it is important to identify SDE genes for all fold changes. If fewer than 12 replicates are used, a superior combination of true positive and false positive performances makes edgeR and DESeq2 the leading tools. For higher replicate numbers, minimizing false positives is more important and DESeq marginally outperforms the other tools.},
author = {Schurch, Nicholas J. and Schofield, Piet{\'{a}} and Gierli{\'{n}}ski, Marek and Cole, Christian and Sherstnev, Alexander and Singh, Vijender and Wrobel, Nicola and Gharbi, Karim and Simpson, Gordon G. and Owen-Hughes, Tom and Blaxter, Mark and Barton, Geoffrey J.},
doi = {10.1261/rna.053959.115},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Schurch et al. - 2016 - How many biological replicates are needed in an RNA-seq experiment and which differential expression tool sho(2).pdf:pdf},
issn = {14699001},
journal = {RNA},
keywords = {Benchmarking,Differential expression,Experimental design,RNA-seq,Replication,Statistical power,Yeast},
month = {jun},
number = {6},
pages = {839--851},
pmid = {27022035},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{How many biological replicates are needed in an RNA-seq experiment and which differential expression tool should you use?}},
volume = {22},
year = {2016}
}
@article{Schmidt2009,
abstract = {*[I report the results of the first detailed phylogenetic study of the ant subfamily Ponerinae, a diverse lineage whose properties make it an attractive model system for investigating social {\&} ecological evolution in ants. Molecular sequence data were obtained from 7 nuclear genes for 86 ponerine taxa, representing all 3 ponerine tribes, 22 of the 28 currently recognized genera, {\&} 14 of the 18 informal subgenera of Pachycondyla, a genus strongly suspected to be non-monophyletic. Divergence dates, historical biogeography, {\&} the origin of reproduction by gamergate workers were estimated on the inferred phylogeny. The most important results of this study include: (1) the monophyly of Ponerinae; (2) the paraphyly of Ponerini; (3) the extensive non-monophyly of Pachycondyla; (4) a rapid basal radiation in Ponerini; (5) resolution of many deeper relationships within Ponerini; (6) a rapid {\&} rich basal radiation in the Odontomachus group; (7) an origin for Ponerinae in the upper Cretaceous, with a major burst of diversification near the K/T boundary; (8) a history of rich {\&} continual ponerine diversification during the Cenozoic; (9) a history of regionalized radiations {\&} frequent faunal exchange between major biogeographic provinces; {\&} (10) support for multiple origins of gamergates in Ponerinae. Ponerine taxonomic classification is revised to reflect both these phylogenetic results {\&} a re-appraisal of ponerine morphological diversity. The monogeneric tribe Thaumatomyrmecini is newly synonymized under Ponerini, {\&} the diverse genus Pachycondyla is fragmented into 15 genera, largely along the lines of its junior synonyms, though 2 new genera are erected: Buniapone (gen.nov.) {\&} Mayaponera (gen.nov.). Some junior synonyms of Pachycondyla are transferred to junior synonym status under other genera: Wadeura as a junior synonym of Cryptopone (syn.nov.), Xiphopelta as a junior synonym of Pseudoponera (syn.nov.), {\&} both Termitopone {\&} Syntermitopone as junior synonyms of Neoponera (syn.nov.). Molecular {\&} morphological justifications for these taxonomic changes are given alongside discussions of phylogenetic relationships. Keys to the world genera of Ponerinae are provided, {\&} morphological diagnoses {\&} species lists are given for each genus. Finally, the available information on ponerine ecology and behavior is reviewed {\&} synthesized.]},
author = {Schmidt, C A},
keywords = {Buniapone,Cryptopone,Formicidae,Mayaponera,Neoponera,Odontomachus,Pachycondyla,Ponerinae,Ponerini,Pseudoponera,Syntermitopone,Termitopone,Thaumatomyrmecini,Wadeura,Xiphopelta,ant,behavior,distribution,ecology,gamergates,genetics,morphology,phylogenetics,scientific,systematics,taxonomy},
number = {May},
pages = {201--250},
title = {{Molecular phylogenetics and taxonomic revision of ponerine ants (Hymenoptera: Formicidae: Ponerinae)}},
volume = {3647},
year = {2009}
}
@article{Lagesen2007,
abstract = {The publication of a complete genome sequence is usually accompanied by annotations of its genes. In contrast to protein coding genes, genes for ribosomal RNA (rRNA) are often poorly or inconsistently annotated. This makes comparative studies based on rRNA genes difficult. We have therefore created computational predictors for the major rRNA species from all kingdoms of life and compiled them into a program called RNAmmer. The program uses hidden Markov models trained on data from the 5S ribosomal RNA database and the European ribosomal RNA database project. A pre-screening step makes the method fast with little loss of sensitivity, enabling the analysis of a complete bacterial genome in less than a minute. Results from running RNAmmer on a large set of genomes indicate that the location of rRNAs can be predicted with a very high level of accuracy. Novel, unannotated rRNAs are also predicted in many genomes. The software as well as the genome analysis results are available at the CBS web server. {\textcopyright} 2007 The Author(s).},
author = {Lagesen, Karin and Hallin, Peter and R{\o}dland, Einar Andreas and St{\ae}rfeldt, Hans Henrik and Rognes, Torbj{\o}rn and Ussery, David W},
doi = {10.1093/nar/gkm160},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lagesen et al. - 2007 - RNAmmer consistent and rapid annotation of ribosomal RNA genes.pdf:pdf},
issn = {03051048},
journal = {Nucleic Acids Research},
number = {9},
pages = {3100--3108},
title = {{RNAmmer: Consistent and rapid annotation of ribosomal RNA genes}},
volume = {35},
year = {2007}
}
