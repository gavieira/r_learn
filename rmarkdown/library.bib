@article{Brandao2008,
abstract = {Simopelta minima (Brand{\~{a}}o, 1989) was originally described based on 4 workers collected in soil samples from a small cocoa plantation in Ilh{\'{e}}us, state of Bahia, northeastern Brazil. In the subsequent years after the description, this cocoa plantation was eliminated {\&} the species was then considered extinct by the Brazilian environmental institutions. The recent rediscovery of S. minima workers in subterranean pitfall trap samples from Vi{\c{c}}osa, state of Minas Gerais, southeastern Brazil, over 1,000-km distant from type locality, suggests that the rarity {\&} vulnerability status of some ant species may be explained by insufficient sampling of adequate microhabitats, in time {\&} space.},
