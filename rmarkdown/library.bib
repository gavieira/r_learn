isbn = {1088601405},
issn = {15731561},
journal = {Journal of Chemical Ecology},
keywords = {Alkylpyrazines,Chiral column,Chirality,Formicidae,Hymenoptera,Invasive pest,Passive behavior response,Pseudomonas cepacia lipase,Pyrazines},
number = {11-12},
pages = {1277--1285},
pmid = {25370918},
