doi = {10.1016/j.tree.2004.07.006},
file = {:home/gabriel/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Graham et al. - 2004 - New developments in museum-based informatics and applications in biodiversity analysis.pdf:pdf},
